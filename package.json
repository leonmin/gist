  "repository": {
    "type": "git",
    "url": "https://github.com/leonmin/gist.git"
  },
  "devDependencies": {
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@types/inquirer": "9.0.8",
    "@types/node": "24.0.11",
    "@types/yargs": "17.0.33",
    "boxen": "8.0.1",
    "chalk": "5.4.1",
    "clipboardy": "4.0.0",
    "inquirer": "12.7.0",
    "lowdb": "7.0.1",
    "yargs": "18.0.0"
