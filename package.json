  "description": "简单的命令行\"片段\"管理器",
  "main": "bin/index.js",
  "scripts": {
    "dev": "nodemon --watch './src/**/*.ts' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/index.ts",
    "build": "tsc"
  },
  "keywords": [
    "cli",
    "snippet",
    "gist",
    "clipboard",
    "command-line"
